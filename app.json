{
  "expo": {
    "name": "BotanicalBuddy",
    "slug": "BotanicalBuddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bb-icon.png",
    "splash": {
      "image": "./assets/images/plant_bg.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "botanical-buddy"
    },
    "android": {
      "package": "com.batzlov.botanicalbuddy",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bb-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
